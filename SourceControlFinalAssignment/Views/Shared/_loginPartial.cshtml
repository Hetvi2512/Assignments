@if (Session["email"] == null)
{
    <ul class="nav navbar-nav navbar-right">
        <li>
            @Html.ActionLink("Sign-up", "Register", "Account", null, null)
        </li>
        <li>
            @Html.ActionLink("Login", "Login", "Account", null, null)
        </li>
    </ul>
}

else
{
    <ul class="nav navbar-nav navbar-right">
        <li>
            @Html.ActionLink("Hello " + Session["email"] , "Index", "Account", null, null)
        </li>
        <li>
            @Html.ActionLink("Logout", "Logout", "Account", null, null)
        </li>
    </ul>
}
